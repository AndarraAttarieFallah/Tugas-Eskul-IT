	<script>
		window.onload=init();

		function init(){
			canvas=document.getElementById('canvas');
			cw =canvas.width =960;
			ch =canvas.height =600;
			pen =canvas.getContext('2d'); 
			console.log(pen);
			pen.beginPath();
			pen.fillStyle = 'green';
			pen.rect(10,10,100,100);
			pen.fill();

			pen.beginPath();
			pen.fillStyle = 'black';
			pen.rect(850,10,100,100);
			pen.fill();

			pen.beginPath();
			pen.fillStyle = 'blue';
			pen.rect(10,490,100,100);
			pen.fill();

			pen.beginPath();
			pen.fillStyle = 'red';
			pen.rect(850,490,100,100);
			pen.fill();

			pen.beginPath();
			pen.fillStyle = 'black';
			pen.rect(430,250,100,100);
			pen.stroke();

			pen.beginPath();
			pen.fillStyle = 'yellow';
			pen.rect(cw/2-50,480,100,100);
			pen.fill();

			pen.beginPath();
			pen.fillStyle = 'pink';
			pen.rect(cw/2-50,480,100,100);
			pen.fill();

			pen.beginPath();
			pen.fillStyle = 'purple';
			pen.rect(cw/2-50,10,100,100);
			pen.fill();

			pen.beginPath();
			pen.fillStyle = 'brown';
			pen.rect(10,ch/2-50,100,100);
			pen.fill();

			pen.beginPath();
			pen.fillStyle = 'aqua';
			pen.rect(850,ch/2-50,100,100);
			pen.fill();

				pen.beginPath();
			pen.fillStyle = 'aqua';
			pen.rect(850,ch/2-50,100,100);
			pen.fill();

			pen.beginPath();
			pen.fillStyle = 'grey';
			pen.rect((cw/2) - (cw/4) - 25,
					(ch/2) - (ch/4) - 25,
				100,100);
			pen.fill();

			pen.beginPath();
			pen.fillStyle = 'maroon';
			pen.rect((cw/2)+(cw/4)-75,
				(ch/2)-(ch/4)-25,100,100);
			pen.fill();

			pen.beginPath();
			pen.fillStyle = 'gold';
			pen.rect((cw/2)+(cw/4)-75,
				(ch/2)+(ch/4)-25,100,100);
			pen.fill();

			pen.beginPath();
			pen.fillStyle = 'gold';
			pen.rect((cw/2)-(cw/4)-25,
				(ch/2)+(ch/4)-25,100,100);
			pen.fill();



		}
